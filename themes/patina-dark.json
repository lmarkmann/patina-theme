    {
      "scope": [
        "storage.type.function",
        "storage.type.class",
        "storage.type.interface",
        "storage.type.enum",
        "storage.type.struct"
      ],
      "settings": {
        "foreground": "#4d9375"
      }
    },
    {
      "scope": [
        "string",
        "string punctuation.section.embedded source",
        "attribute.value"
      ],
      "settings": {
        "foreground": "#c98a7d"
      }
    },
    {
      "scope": [
        "string.regexp",
        "source.regexp"
