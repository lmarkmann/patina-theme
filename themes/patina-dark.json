      "settings": {
        "foreground": "#c99076"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#c98a7d"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#80a665"
      }
    },
    {
      "scope": [
        "entity.name.function.macro",
        "support.function.macro"
      ],
      "settings": {
        "foreground": "#cb7676"
      }
    },
    {
      "scope": [
        "entity",
        "entity.name"
      ],
      "settings": {
