  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#3e918f",
    "property": "#8a6e3e",
    "interface": "#3e918f",
    "type": "#3e918f",
    "class": "#3e918f",
    "enum": "#3e918f",
    "enumMember": "#3e918f",
    "parameter": "#8c8a35",
    "variable.readonly": "#a66840",
    "function": "#5c8a4e",
    "method": "#5c8a4e",
    "macro": "#ab5959",
    "keyword": "#3e7a5e"
