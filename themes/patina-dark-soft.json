  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#5a9e9c",
    "property": "#af8d62",
    "interface": "#5a9e9c",
    "type": "#5a9e9c",
    "class": "#5a9e9c",
    "enum": "#5a9e9c",
    "enumMember": "#5a9e9c",
    "parameter": "#ad9e5e",
    "variable.readonly": "#b88068",
    "function": "#78985e",
    "method": "#78985e",
    "macro": "#b86e6e",
    "keyword": "#4a8a6e"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#6e7e6e"
      }
    },
    {
      "scope": [
        "punctuation",
        "delimiter",
        "delimiter.bracket",
        "meta.brace",
        "meta.tag.block.any.html",
        "meta.tag.inline.any.html",
        "meta.tag.structure.input.void.html",
        "meta.type.annotation",
        "storage.type.function.arrow",
        "meta.objectliteral.ts"
      ],
      "settings": {
        "foreground": "#787878"
      }
    },
    {
      "scope": [
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#b8806f88"
      }
    },
    {
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#b86e6e"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type.class.jsdoc"
