    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment.compound",
        "keyword.operator.rest",
        "keyword.operator.spread",
        "keyword.operator.ternary",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#b86e6e"
